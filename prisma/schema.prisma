model sysdiagrams {
  name         String @db.NVarChar(128)
  principal_id Int
  diagram_id   Int    @id(map: "PK__sysdiagr__C2B05B6111070C4D") @default(autoincrement())
  version      Int?
  definition   Bytes?

  @@unique([principal_id, name], map: "UK_principal_name")
}

model User {
  id            String    @id
  email         String?   @unique
  name          String?
  image         String?   @map("pict")
  // emailVerified DateTime? // if applicable
  role          String?
  div_cd          String?
  dept_cd          String?

  accounts      Account[]
  sessions      Session[]

  @@map("users")
}

model Account {
  id                String   @id @default(cuid())
  userId            String   @map("user_id")
  type              String
  provider          String
  providerAccountId String
  refresh_token     String?
  access_token      String?
  expires_at        Int?     // Change to DateTime? if your column is a date
  token_type        String?
  scope             String?
  id_token          String?
  session_state     String?

  user              User?    @relation(fields: [userId], references: [id])

  @@unique([provider, providerAccountId])
  @@map("accounts")
}

model Session {
  id           String   @id @default(cuid())
  sessionToken String   @unique @map("session_token")
  userId       String   @map("user_id")
  expires      DateTime

  user         User?    @relation(fields: [userId], references: [id])
  @@map("sessions")
}

model VerificationToken {
  identifier   String
  token        String   @unique
  expires      DateTime

  @@unique([identifier, token])
  @@map("verification_tokens")
}